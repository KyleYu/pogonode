    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch",
            "program": "${workspaceRoot}\\app.js",
            "cwd": "${workspaceRoot}",
            "env": {
				"NODE_ENV": "development",
                "NODE_TLS_REJECT_UNAUTHORIZED": 0
            },
            "args": [
                "--trace-warnings"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch  Captcha Helper",
            "program": "${workspaceRoot}\\captcha\\app.js",
            "cwd": "${workspaceRoot}",
            "env": {
				"NODE_ENV": "development",
                "NODE_TLS_REJECT_UNAUTHORIZED": 0
            },
            "args": [
                "--trace-warnings"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Debug",
            "program": "${workspaceRoot}\\debug.js",
            "cwd": "${workspaceRoot}",
            "env": {
				"NODE_ENV": "development"
            },
            "args": [
                "--trace-warnings"
            ]

        }
    ]
}