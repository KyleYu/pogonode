{
    "root": true,
    "extends": "google",
    "env": {
        "es6": true,
        "browser": false,
        "node": true
    },
    "rules": {
        "arrow-parens": 0,
        "padded-blocks": "off",
        "max-len": [1, {
            "tabWidth": 4,
            "code": 120,
            "comments": 100
        }],
        "require-jsdoc": 0
    }
}